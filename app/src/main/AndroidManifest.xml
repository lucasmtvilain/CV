<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lucas.cvlucasvilain">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lucasvilain"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.CompetenceActivity"
            android:label="@string/title_activity_competence"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.FormationActivity"
            android:label="@string/title_activity_formation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.ExperienceActivity"
            android:label="@string/title_activity_experience"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>